---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Dont Laugh's Blog</title>
	<link href="https://dont-laugh.github.io/blog/atom.xml" rel="self" />
	<link href="https://dont-laugh.github.io/blog" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://dont-laugh.github.io/blog</id>
	<author>
		<name>Dont Laugh</name>
		<email>2387471378@qq.com</email>
	</author>
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://dont-laugh.github.io{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://dont-laugh.github.io{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>